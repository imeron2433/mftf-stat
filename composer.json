    "consolidation/robo": "*",
    "codeception/codeception": "*",
    "mustache/mustache": "*",
    "fzaninotto/faker": "*",
    "flow/jsonpath": "*",
    "doctrine/lexer": "*",
    "doctrine/annotations": "*",
    "doctrine/collections": "*",
    "doctrine/instantiator": "*",
    "phpcollection/phpcollection": "*",
    "jms/parser-lib": "*",
    "jms/metadata": "*",
    "jms/serializer": "*",
    "epfremme/swagger-php": "*"
