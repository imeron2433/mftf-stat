{
  "name": "magento/mftf-stat_cli",
  "description": "Magento 2 (Open Source)",
  "type": "project",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
